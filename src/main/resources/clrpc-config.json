{
    "registry": "zookeeper://127.0.0.1:2181/clrpc?session-timeout=5000",
    "atomicity": "zookeeper://127.0.0.1:2181/clrpc?session-timeout=5000",
    "logger": "zookeeper://127.0.0.1:2181/clrpc?session-timeout=5000",

    "meta.provider.*": {},
    "meta.consumer.*": {},

    "provider.port": 0,
    "provider.thread.boss": 1,
    "provider.thread.worker": 4,
    "provider.channel.handler-factory": null,

    "consumer.wait-time": 5000,
    "consumer.thread.worker": 4,
    "consumer.retry.check-period" : 3000,
    "consumer.retry.initial-threshold" : 3000,
    "consumer.fallback.max-retry": -1,
    "consumer.channel.handler-factory": null,

    "service.thread-pool.core-size": 5,
    "service.thread-pool.max-size": 10,
    "service.thread-pool.keep-alive": 1000,
    "service.thread-pool.queue": 10
}