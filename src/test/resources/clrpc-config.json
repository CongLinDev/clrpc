{
    "zookeeper.provider.address": "127.0.0.1:2181",
    "zookeeper.provider.root-path": "/clrpc",
    "zookeeper.provider.session-timeout": 5000,
    "zookeeper.consumer.address": "127.0.0.1:2181",
    "zookeeper.consumer.root-path": "/clrpc",
    "zookeeper.consumer.session-timeout": 5000,
    "zookeeper.atomicity.address": "127.0.0.1:2181",
    "zookeeper.atomicity.root-path": "/clrpc",
    "zookeeper.atomicity.session-timeout": 5000,
    "zookeeper.logger.address": "127.0.0.1:2181",
    "zookeeper.logger.root-path": "/clrpc",
    "zookeeper.logger.session-timeout": 5000,

    "meta.provider.*": {},
    "meta.consumer.*": {},

    "provider.port": 0,
    "provider.thread.boss": 1,
    "provider.thread.worker": 4,
    "provider.channel.handler-factory": null,
    "provider.logger.traffic.enable": false,

    "consumer.wait-time": 5000,
    "consumer.thread.worker": 4,
    "consumer.retry.check-period" : 3000,
    "consumer.retry.initial-threshold" : 3000,
    "consumer.fallback.max-retry": -1,
    "consumer.channel.handler-factory": null,
    "consumer.logger.traffic.enable": false,

    "service.thread-pool.core-size": 5,
    "service.thread-pool.max-size": 10,
    "service.thread-pool.keep-alive": 1000,
    "service.thread-pool.queue": 10
}